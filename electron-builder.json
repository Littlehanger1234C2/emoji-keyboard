{
  "appId": "tk.ozymandias.emoji-keyboard",
  "productName": "emoji-keyboard",
  "copyright": "Â© 2020 ${author}",
  "directories": {
    "buildResources": "dist/resources",
    "output": "dist/release/"
  },
  "electronDownload": {
    "version": "8.0.1"
  },
  "nodeVersion": "${current}",
  "files": [
      "**/*",
      "!**/*.ts",
      "!**/*.py",
      "!.*",
      "!*.json",
      "!*.md",
      "!*.code-workspace",
      "!*.webpack.*",
      "!LICENSE",
      "!package.json",
      "!package-lock.json",
      "!pyproject.toml",
      "!poetry.lock",
      "!src",
      "!src_py",
      "!.venv",
      "!scripts",
      "!sources",
      "!karma.conf.js",
      "!.vscode"
  ],
  "linux": {
    "icon": "dist/resources/icon",
    "category": "Utility",
    "target": [
      "AppImage"
    ]
  },
  "appImage": {
    "license": "LICENSE"
  },
  "extraResources": [
    {
      "from": "dist/resources/main.pyz",
      "to": "main.pyz"
    }
  ],
  "asar": true,
  "asarUnpack": [
    "dist/angular/assets/data/emoji.json",
    "dist/angular/assets/data/default.ini"
  ]
}
